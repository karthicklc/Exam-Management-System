# Example environment variables for the backend
# Copy this file to .env and fill the real secrets before running.

# Django
SECRET_KEY=your-django-secret-key
DEBUG=False
ALLOWED_HOSTS=localhost,127.0.0.1

# Supabase/Postgres connection pieces (optional)
SUPABASE_DB_NAME=your_db_name
SUPABASE_DB_USER=your_db_user
SUPABASE_DB_PASSWORD=your_db_password
SUPABASE_DB_HOST=your_db_host
SUPABASE_DB_PORT=5432

# Full DATABASE_URL (this will be used if present). Example:
# DATABASE_URL=postgres://<user>:<password>@<host>:<port>/<dbname>
DATABASE_URL=

# Other recommended vars
# PORT for the web server
PORT=8000
